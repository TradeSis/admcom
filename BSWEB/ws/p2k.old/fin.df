  VALEXP "can-find(empre of titulo)"
  VALMSG "Empresa nao cadastrada."
  HELP "Informe o Codigo da Empresa."
  ORDER 10

ADD FIELD "titnum" OF "log_titluc" AS character 
  FORMAT "x(12)"
  INITIAL ""
  LABEL "Titulo"
  POSITION 3
  MAX-WIDTH 24
  VALEXP "titnum <> """""
  VALMSG "Numero do Titulo nao pode ser brancos"
  HELP "Informe o Numero do Titulo."
  ORDER 50

ADD FIELD "titpar" OF "log_titluc" AS integer 
  FORMAT ">>9"
  INITIAL "1"
  LABEL "Parcela"
  POSITION 4
  MAX-WIDTH 4
  COLUMN-LABEL "PC"
  VALEXP "titpar > 0"
  VALMSG "Numero da Parcela do Titulo nao pode ser zero."
  HELP "Informe o Numero da Parcela do Titulo."
  ORDER 60

ADD FIELD "etbcod" OF "log_titluc" AS integer 
  FORMAT ">>>9"
  INITIAL "1"
  LABEL "Estabelecimento"
  POSITION 5
  MAX-WIDTH 4
  COLUMN-LABEL "Est."
  VALEXP "can-find(estab of titulo)"
  VALMSG "Estabelecimento nao cadastrado."
  HELP "Informe o Codigo do Estabelecimento do Titulo."
  ORDER 80

ADD FIELD "cobcod" OF "log_titluc" AS integer 
  FORMAT ">9"
  INITIAL "1"
  LABEL "Cobranca"
  POSITION 6
  MAX-WIDTH 4
  COLUMN-LABEL "Cob"
  VALEXP "can-find(cobra of titulo)"
  VALMSG "Tipo de Cobranca nao Cadastrada."
  HELP "Informe o Tipo de Cobranca, conforme Tabela."
  ORDER 150

ADD FIELD "titdtven" OF "log_titluc" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Vencimento"
  POSITION 7
  MAX-WIDTH 4
  VALEXP "titdtven >= input titdtemi"
  VALMSG "Data de Vencimento deve ser posterior a Data de Emissao."
  HELP "Informe a Data de Vencimento do Titulo."
  ORDER 100

ADD FIELD "titvlcob" OF "log_titluc" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Valor Cobrado"
  POSITION 8
  MAX-WIDTH 17
  VALEXP "titvlcob > 0"
  VALMSG "Valor do Titulo nao pode ser zero."
  HELP "Informe o Valor do Titulo."
  DECIMALS 2
  ORDER 110

ADD FIELD "titvldes" OF "log_titluc" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Valor Desconto"
  POSITION 9
  MAX-WIDTH 17
  VALEXP "titvldes = 0 or titvldes < input titvlcob"
  VALMSG "Valor do Desconto deve ser menor que valor do Titulo."
  HELP "Informe o Valor do Desconto por Antecipacao de Pagamento."
  DECIMALS 2
  ORDER 130

ADD FIELD "titdtdes" OF "log_titluc" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Data Desconto"
  POSITION 10
  MAX-WIDTH 4
  COLUMN-LABEL "Dt.Descon."
  VALEXP "titdtdes = ? or (titdtdes >= input titdtemi and titdtdes <= input titdtven)"
  VALMSG "Data p/Desconto deve ser maior que Emissao e menor que Vencim."
  HELP "Informe a Data Limite para pagamento com Desconto."
  ORDER 120

