ADD TABLE "atend"
  AREA "dados"
  DESCRIPTION "Registros de Atendimento Setor x Classe"
  DUMP-NAME "atend"

ADD FIELD "clacod" OF "atend" AS integer 
  FORMAT ">>>>>>>>9"
  INITIAL "0"
  LABEL "Classe"
  POSITION 2
  MAX-WIDTH 4
  VALEXP "{dv.v ""clacod""} and can-find(clase of atend)"
