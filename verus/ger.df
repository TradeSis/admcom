ADD TABLE "CPFis"
  AREA "dados"
  DESCRIPTION "Cadastro de Clientes Pessoa Física"
  DUMP-NAME "cpfis"

ADD FIELD "CliFor" OF "CPFis" AS integer 
  DESCRIPTION "Código Interno do Agente Comercial"
  FORMAT ">>>>>>>999"
  INITIAL "0"
  LABEL "Agente Comercial"
  MAX-WIDTH 4
  COLUMN-LABEL "Cod."
  VALEXP "can-find(AgCom of CPFis)"
  VALMSG "Codigo do Agente Comercial não Cadastrado"
  HELP "Código Interno do Agente Comercial"
  ORDER 20
  MANDATORY

ADD FIELD "UFed" OF "CPFis" AS character 
  DESCRIPTION "Unidade da Federacao"
  FORMAT "!!"
  INITIAL ""
  LABEL "UF"
  MAX-WIDTH 4
  COLUMN-LABEL "UF"
  HELP "Unidade da Federacao"
  ORDER 50
  MANDATORY

ADD FIELD "DtNasc" OF "CPFis" AS date 
  DESCRIPTION "Indica a Data de Nascimento"
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Dt.Nasc."
  MAX-WIDTH 4
  COLUMN-LABEL "Dt.Nasc."
  VALEXP "DtNasc <= today and ((today - DtNasc) >= 6570) and ((today - DtNasc) <= 35600)"
  VALMSG "Data de Nascimento Inválida"
  HELP "Data de Nascimento"
  ORDER 30
  MANDATORY

ADD FIELD "LocNasc" OF "CPFis" AS character 
  DESCRIPTION "Indica a Localidade de nascimento"
  FORMAT "x(20)"
  INITIAL ""
  LABEL "Local"
  MAX-WIDTH 40
  COLUMN-LABEL "Localidade"
  VALEXP "LocNasc <> "" """
  VALMSG "Localidade de Nascimento é Obrigatório"
  HELP "Localidade de Nascimento"
  ORDER 40
  MANDATORY

ADD FIELD "EstCivil" OF "CPFis" AS character 
  DESCRIPTION "Indica o Estado Civil"
  FORMAT "!"
  INITIAL ""
  LABEL "Est.Civil"
  MAX-WIDTH 2
  COLUMN-LABEL "Est.Civil"
  VALEXP "LookUp(EstCivil,""C,D,O,S,V"") <> 0"
  VALMSG "Estado Civil Inválido"
  HELP "Estado Civil (C,D,O,S,V)"
  ORDER 60
  MANDATORY

ADD FIELD "Conjuge" OF "CPFis" AS character 
  DESCRIPTION "Nome do Conjuge"
  FORMAT "x(30)"
  INITIAL ""
  LABEL "Conjuge"
  MAX-WIDTH 60
  COLUMN-LABEL "Conjuge"
  VALEXP "((input EstCivil = ""C"" or input EstCivil = ""O"") and Conjuge <> "" "") or (Conjuge = "" "" and not (input EstCivil = ""C""or input EstCivil = ""O""))"
  VALMSG "Nome do Conjuge Obrigatório para Est.Civil Casado/Outros"
  HELP "Nome do Comjuge"
  ORDER 70

ADD FIELD "ConjTrab" OF "CPFis" AS logical 
  DESCRIPTION "Indica se o Conjuge Trabalha"
  FORMAT "SIM/NÃO"
  INITIAL "no"
  LABEL "Trab."
  MAX-WIDTH 1
  COLUMN-LABEL "Trab."
  HELP "Conjuge Trabalha"
  ORDER 80

ADD FIELD "NomPai" OF "CPFis" AS character 
  FORMAT "x(35)"
  INITIAL ""
  LABEL "Pai"
  MAX-WIDTH 70
  HELP "Nome do Pai"
  ORDER 510

ADD FIELD "Nacion" OF "CPFis" AS character 
  DESCRIPTION "Nacionalidade"
  FORMAT "x(12)"
  INITIAL "BRASILEIRO"
