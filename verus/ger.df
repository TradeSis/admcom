  INITIAL "0"
  LABEL "Codigo Controle"
  MAX-WIDTH 4
  COLUMN-LABEL "Cod."
  VALEXP "cnt-cod <> 0"
  VALMSG "Codigo Controle nao pode ser zero."
  HELP "Informe o Codigo do Controle de Transacoes."
  ORDER 5

ADD FIELD "cnt-pro" OF "controle" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Programa"
  MAX-WIDTH 16
  VALEXP "if cnt-tip then cnt-pro = """" else cnt-pro <> """""
  VALMSG "Programa associado para tipo invalido."
  HELP "Informe o nome do Programa associado a transacao."
  ORDER 20

ADD FIELD "cnt-per" OF "controle" AS character 
  FORMAT "x(60)"
  INITIAL "*"
  LABEL "Permissoes"
  MAX-WIDTH 120
  HELP "Informe as Permissoes de execussao do Programa."
  ORDER 30

ADD FIELD "cnt-msg" OF "controle" AS character 
  FORMAT "x(17)"
  INITIAL ""
  LABEL "Mensagem Menu"
  MAX-WIDTH 34
  VALEXP "integer(string(cnt-msg,""99"")) = cnt-ord"
  VALMSG "Mensagem Menu deve comecar pelo numero de ordenacao."
  HELP "Selecione a opcao desejada."
  ORDER 40

ADD FIELD "cnt-tip" OF "controle" AS logical 
  FORMAT "Menu/Programa"
  INITIAL "yes"
  LABEL "Tipo"
  MAX-WIDTH 1
  HELP "Informe o Tipo de Controle : [M] - Menu  [P] - Programa"
  ORDER 50

ADD FIELD "cnt-niv" OF "controle" AS integer 
  FORMAT "9"
  INITIAL "0"
  LABEL "Nivel"
  MAX-WIDTH 4
  HELP "Informe o Nivel do Controle na hierarquia."
  ORDER 60

ADD FIELD "cnt-sup" OF "controle" AS integer 
  FORMAT ">>>9"
  INITIAL "0"
  LABEL "Superior"
  MAX-WIDTH 4
  VALEXP "cnt-sup = 0 or can-find(controle where cnt-cod = cnt-sup)"
  VALMSG "Controle Superior nao cadastrado."
  HELP "Informe o Codigo do Controle superior na hierarquia."
  ORDER 75

ADD FIELD "cnt-tit" OF "controle" AS character 
  FORMAT "x(40)"
  INITIAL ""
  LABEL "Titulo"
  MAX-WIDTH 80
  HELP "Informe o Titulo da Transacao para composicao de Cabecalho."
  ORDER 85

ADD FIELD "cnt-par" OF "controle" AS character 
  FORMAT "x"
  INITIAL ""
  LABEL "Parametro"
  MAX-WIDTH 2
  COLUMN-LABEL "Par."
  HELP "Informe o Parametro a ser passado para o Programa."
  ORDER 95

ADD FIELD "cnt-sub" OF "controle" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Subrotina"
  MAX-WIDTH 16
  HELP "Informe o Nome da Subrotina a ser executada para este Controle."
  ORDER 105

ADD FIELD "cnt-ord" OF "controle" AS integer 
  FORMAT ">9"
  INITIAL "0"
  LABEL "Ordem"
  MAX-WIDTH 4
  ORDER 115
  MANDATORY

ADD FIELD "cnt-are" OF "controle" AS character 
  FORMAT "x(2)"
  INITIAL ""
  LABEL "Area"
  MAX-WIDTH 4
  HELP "Informe a Area."
  ORDER 125

ADD FIELD "cnt-help" OF "controle" AS character 
  FORMAT "x(78)"
  INITIAL ""
  LABEL "Help"
  MAX-WIDTH 2528
  HELP "Digite (F7) para salvar/recuperar tela de Help."
  EXTENT 16
  ORDER 135

ADD INDEX "icnt-cod" ON "controle" 
  AREA "indices"
  UNIQUE
  PRIMARY
  INDEX-FIELD "cnt-cod" ASCENDING 

ADD INDEX "controle1" ON "controle" 
  AREA "indices"
  UNIQUE
  INDEX-FIELD "cnt-sup" ASCENDING 
  INDEX-FIELD "cnt-ord" ASCENDING 

