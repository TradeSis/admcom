  ORDER 140

ADD FIELD "perc_pagas" OF "novcampanha" AS decimal 
  FORMAT ">>9.99"
  INITIAL "0"
  LABEL "ppg"
  MAX-WIDTH 17
  DECIMALS 2
  ORDER 60

ADD FIELD "qtd_pagas" OF "novcampanha" AS integer 
  FORMAT ">>9"
  INITIAL "0"
  LABEL "qpg"
  MAX-WIDTH 4
  ORDER 70

ADD FIELD "tpcontrato" OF "novcampanha" AS character 
  FORMAT "x(1)"
  INITIAL ""
