  LABEL "aberto"
  MAX-WIDTH 17
  DECIMALS 2
  ORDER 180

ADD FIELD "vlr_parcela" OF "novcampanha" AS decimal 
  FORMAT ">>>9.99"
  INITIAL "0"
  LABEL "parcel"
  MAX-WIDTH 17
  DECIMALS 2
  ORDER 100

ADD FIELD "vlr_total" OF "novcampanha" AS decimal 
  FORMAT ">>>>>9.99"
  INITIAL "0"
  LABEL "total"
  MAX-WIDTH 17
  DECIMALS 2
  ORDER 50

ADD FIELD "dias_venc" OF "novcampanha" AS integer 
  FORMAT "->>>>9"
  INITIAL "0"
  LABEL "venc"
  MAX-WIDTH 4
  ORDER 190

ADD FIELD "camcod" OF "novcampanha" AS integer 
  FORMAT ">>>>9"
  INITIAL ?
  MAX-WIDTH 4
  ORDER 1

ADD INDEX "campanha" ON "novcampanha" 
  AREA "indices"
  UNIQUE
  PRIMARY
  INDEX-FIELD "camcod" ASCENDING 

ADD INDEX "novcamvig" ON "novcampanha" 
  AREA "indices"
  INDEX-FIELD "dtfim" ASCENDING 
  INDEX-FIELD "dtini" ASCENDING 

ADD TABLE "novestab"
