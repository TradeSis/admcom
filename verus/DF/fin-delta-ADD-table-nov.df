ADD TABLE "novcampanha"
  AREA "dados"
  DUMP-NAME "novcampanha"

ADD FIELD "camnom" OF "novcampanha" AS character 
  FORMAT "x(12)"
  INITIAL ""
  LABEL "campanha"
  MAX-WIDTH 30
  ORDER 20

ADD FIELD "dtini" OF "novcampanha" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "inicio"
  MAX-WIDTH 4
  ORDER 30

ADD FIELD "dtfim" OF "novcampanha" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "final"
