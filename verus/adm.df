  INITIAL "0"
  LABEL "Empresa"
  MAX-WIDTH 4
  COLUMN-LABEL "Emp."
  HELP "Informe o Codigo da Empresa."
  ORDER 10

ADD FIELD "titnum" OF "titudesp" AS character 
  FORMAT "x(12)"
  INITIAL ""
  LABEL "titudesp"
  MAX-WIDTH 24
  HELP "Informe o Numero do titudesp."
  ORDER 50

ADD FIELD "titpar" OF "titudesp" AS integer 
  FORMAT ">>9"
  INITIAL "1"
  LABEL "Parcela"
  MAX-WIDTH 4
  COLUMN-LABEL "PC"
  HELP "Informe o Numero da Parcela do titudesp."
  ORDER 60

ADD FIELD "etbcod" OF "titudesp" AS integer 
  FORMAT ">>>9"
  INITIAL "1"
  LABEL "Estabelecimento"
  MAX-WIDTH 4
  COLUMN-LABEL "Est."
  HELP "Informe o Codigo do Estabelecimento do titudesp."
  ORDER 80

ADD FIELD "cobcod" OF "titudesp" AS integer 
  FORMAT ">9"
  INITIAL "1"
  LABEL "Cobranca"
  MAX-WIDTH 4
  COLUMN-LABEL "Cob"
  HELP "Informe o Tipo de Cobranca, conforme Tabela."
  ORDER 150

ADD FIELD "titdtven" OF "titudesp" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Vencimento"
  MAX-WIDTH 4
  VALEXP "titdtven >= input titdtemi"
  VALMSG "Data de Vencimento deve ser posterior a Data de Emissao."
  HELP "Informe a Data de Vencimento do titudesp."
  ORDER 100

ADD FIELD "titvlcob" OF "titudesp" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Valor Cobrado"
  MAX-WIDTH 17
  VALEXP "titvlcob > 0"
  VALMSG "Valor do titudesp nao pode ser zero."
  HELP "Informe o Valor do titudesp."
  DECIMALS 2
  ORDER 110

ADD FIELD "titvldes" OF "titudesp" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Valor Desconto"
  MAX-WIDTH 17
  VALEXP "titvldes = 0 or titvldes < input titvlcob"
  VALMSG "Valor do Desconto deve ser menor que valor do titudesp."
  HELP "Informe o Valor do Desconto por Antecipacao de Pagamento."
  DECIMALS 2
  ORDER 130

ADD FIELD "titdtdes" OF "titudesp" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Data Desconto"
  MAX-WIDTH 4
  COLUMN-LABEL "Dt.Descon."
  VALEXP "titdtdes = ? or (titdtdes >= input titdtemi and titdtdes <= input titdtven)"
  VALMSG "Data p/Desconto deve ser maior que Emissao e menor que Vencim."
  HELP "Informe a Data Limite para pagamento com Desconto."
  ORDER 120

ADD FIELD "titvljur" OF "titudesp" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Juro Diario"
  MAX-WIDTH 17
  HELP "Informe o Valor do Juro por Dia de Atraso."
  DECIMALS 2
  ORDER 140

ADD FIELD "bancod" OF "titudesp" AS integer 
  FORMAT ">>9"
  INITIAL "0"
  LABEL "Banco Cobrador"
  MAX-WIDTH 4
  COLUMN-LABEL "Bco."
  VALEXP "can-find(banco of titudesp)"
  VALMSG "Banco nao Cadastrado."
  HELP "Informe o Codigo do Banco Cobrador do titudesp."
  ORDER 160

ADD FIELD "agecod" OF "titudesp" AS character 
  FORMAT "x(8)"
  INITIAL "0"
  LABEL "Agen. Cobrador"
  MAX-WIDTH 16
  COLUMN-LABEL "Agencia"
  VALEXP "can-find(agenc of titudesp) or agenc.agecod = ""0"""
  VALMSG "Agencia Cobradora nao cadastrada."
  HELP "Informe a Agencia Bancaria cobradora do titudesp."
  ORDER 170

ADD FIELD "titdtpag" OF "titudesp" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Data Pagamento"
  MAX-WIDTH 4
  COLUMN-LABEL "Data Paga."
  VALEXP "titdtpag >= titdtemi"
  VALMSG "Data de Pagamento do titudesp nao deve ser inferior a Emissao."
  HELP "Informe a Data de Pagamento do titudesp."
  ORDER 180

ADD FIELD "titdesc" OF "titudesp" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Desconto"
  MAX-WIDTH 17
  HELP "Informe o Valor do Desconto Concedido."
  DECIMALS 2
  ORDER 190

ADD FIELD "titjuro" OF "titudesp" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Juros"
  MAX-WIDTH 17
  HELP "Informe o Valor do Juro Atribuido."
  DECIMALS 2
  ORDER 200

ADD FIELD "titbanpag" OF "titudesp" AS integer 
  FORMAT ">>9"
  INITIAL "0"
  LABEL "Banco Pagador"
  MAX-WIDTH 4
  COLUMN-LABEL "BP"
  VALEXP "can-find(banco where banco.bancod = titbanpag)"
  VALMSG "Banco Pagador naop cadastrado."
  HELP "Informe o Codigo do Banco Pagador."
  ORDER 210

ADD FIELD "titagepag" OF "titudesp" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Agen.Pagadora"
  MAX-WIDTH 16
  COLUMN-LABEL "Age.Pag."
  VALEXP "can-find(agenc where agenc.bancod = titbanpag and agenc.agecod = titagepag )"
  VALMSG "Agencia Pagadora nao cadastrada."
  HELP "Informe o Codigo da Agencia Pagadora."
  ORDER 220

ADD FIELD "modcod" OF "titudesp" AS character 
  FORMAT "!!!"
  INITIAL "CRE"
  LABEL "Modalidade"
  MAX-WIDTH 6
  COLUMN-LABEL "MT"
  VALEXP "can-find(modal of titudesp)"
  VALMSG "Modalidade de titudesp nao cadastrada."
  HELP "Informe a Sigla da Modalidade de titudesp."
  ORDER 20

ADD FIELD "titnat" OF "titudesp" AS logical 
  FORMAT "Pagar/Receber"
  INITIAL "no"
  LABEL "Natureza"
  MAX-WIDTH 1
  COLUMN-LABEL "Pag/Rec"
  HELP "Informe a Natureza do titudesp : [ P ]-Pagar  [ R ]-Receber"
  ORDER 70

ADD FIELD "titchepag" OF "titudesp" AS character 
  FORMAT "x(15)"
  INITIAL ""
  LABEL "Cheque Pagador"
  MAX-WIDTH 30
  HELP "Informe o Numero de Cheque vinculado ao titudesp."
  ORDER 230

ADD FIELD "titdtemi" OF "titudesp" AS date 
  FORMAT "99/99/9999"
  INITIAL "today"
  LABEL "Emissao"
  MAX-WIDTH 4
  VALEXP "titdtemi <= today"
  VALMSG "Data de Emissao do titudesp deve ser menor que data de hoje."
  HELP "Informe a Data de Emissao do titudesp."
  ORDER 90

ADD FIELD "titsit" OF "titudesp" AS character 
  FORMAT "!!!"
  INITIAL "IMP"
  LABEL "Situacao"
  MAX-WIDTH 6
  COLUMN-LABEL "Sit"
  VALEXP "lookup(titsit,""LIB,BLO,PAG,PAR"") <> 0"
  VALMSG "Situacao Invalida."
  HELP "Informe a Situacao: LIBerada, BLOqueada, PAGa ou PARcial"
  ORDER 240

ADD FIELD "CliFor" OF "titudesp" AS integer 
  FORMAT ">>>>>>>999"
  INITIAL ?
  LABEL "Cli/For"
  MAX-WIDTH 4
  HELP "Codigo do Cliente/Fornecedor"
  ORDER 40

ADD FIELD "titvlpag" OF "titudesp" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Valor Pago"
  MAX-WIDTH 17
  HELP "Informe o Valor Liquido Pago."
  DECIMALS 2
  ORDER 205

ADD FIELD "titnumger" OF "titudesp" AS character 
  FORMAT "x(12)"
  INITIAL ""
  LABEL "titudesp Gerador"
  MAX-WIDTH 24
  HELP "Informe o Numero do titudesp Gerador."
  ORDER 250

ADD FIELD "titparger" OF "titudesp" AS integer 
  FORMAT ">>9"
  INITIAL "0"
  LABEL "Parcela Geradora"
  MAX-WIDTH 4
  COLUMN-LABEL "PG"
  HELP "Informe o Numero da Parcela Geradora."
  ORDER 260

ADD FIELD "titobs" OF "titudesp" AS character 
  FORMAT "x(78)"
  INITIAL ""
