  FORMAT ">>>9"
  INITIAL "0"
  LABEL "Empresa"
  MAX-WIDTH 4
  COLUMN-LABEL "Emp."
  VALEXP "can-find(empre of tituarqm)"
  VALMSG "Empresa nao cadastrada."
  HELP "Informe o Codigo da Empresa."
  ORDER 10

ADD FIELD "titnum" OF "tituarqm" AS character 
  FORMAT "x(12)"
  INITIAL ""
  LABEL "Titulo"
  MAX-WIDTH 24
  VALEXP "titnum <> """""
  VALMSG "Numero do Titulo nao pode ser brancos"
  HELP "Informe o Numero do Titulo."
  ORDER 50

