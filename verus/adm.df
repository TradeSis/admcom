  LABEL "Hora Movto"
  MAX-WIDTH 16
  HELP "Informe a Hora do Movimento."
  ORDER 300

ADD FIELD "vencod" OF "titudesp" AS integer 
  FORMAT ">>>>9"
  INITIAL "0"
  LABEL "Vendedor"
  MAX-WIDTH 4
  COLUMN-LABEL "Ven"
  HELP "Informe o Codigo do Vendedor."
  ORDER 310

ADD FIELD "etbCobra" OF "titudesp" AS integer 
  FORMAT ">>>9"
  INITIAL ?
  LABEL "Estab. Cobrador"
  MAX-WIDTH 4
  COLUMN-LABEL "Est.Cob"
