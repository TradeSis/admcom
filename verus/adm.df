  FORMAT ">>9"
  INITIAL "1"
  LABEL "Parcela"
  MAX-WIDTH 4
  COLUMN-LABEL "PC"
  VALEXP "titpar > 0"
  VALMSG "Numero da Parcela do Titulo nao pode ser zero."
  HELP "Informe o Numero da Parcela do Titulo."
  ORDER 60

ADD FIELD "etbcod" OF "tituarqm" AS integer 
  FORMAT ">>>9"
  INITIAL "1"
  LABEL "Estabelecimento"
  MAX-WIDTH 4
  COLUMN-LABEL "Est."
  VALEXP "can-find(estab of tituarqm)"
  VALMSG "Estabelecimento nao cadastrado."
  HELP "Informe o Codigo do Estabelecimento do Titulo."
  ORDER 80

ADD FIELD "cobcod" OF "tituarqm" AS integer 
  FORMAT ">9"
  INITIAL "1"
  LABEL "Cobranca"
  MAX-WIDTH 4
  COLUMN-LABEL "Cob"
  VALEXP "can-find(cobra of tituarqm)"
  VALMSG "Tipo de Cobranca nao Cadastrada."
  HELP "Informe o Tipo de Cobranca, conforme Tabela."
  ORDER 150

ADD FIELD "titdtven" OF "tituarqm" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Vencimento"
  MAX-WIDTH 4
  VALEXP "titdtven >= input titdtemi"
  VALMSG "Data de Vencimento deve ser posterior a Data de Emissao."
  HELP "Informe a Data de Vencimento do Titulo."
  ORDER 100

ADD FIELD "titvlcob" OF "tituarqm" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Valor Cobrado"
  MAX-WIDTH 17
  VALEXP "titvlcob > 0"
  VALMSG "Valor do Titulo nao pode ser zero."
  HELP "Informe o Valor do Titulo."
  DECIMALS 2
  ORDER 110

ADD FIELD "titvldes" OF "tituarqm" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
  INITIAL "0"
  LABEL "Valor Desconto"
  MAX-WIDTH 17
  VALEXP "titvldes = 0 or titvldes < input titvlcob"
  VALMSG "Valor do Desconto deve ser menor que valor do Titulo."
  HELP "Informe o Valor do Desconto por Antecipacao de Pagamento."
  DECIMALS 2
  ORDER 130

ADD FIELD "titdtdes" OF "tituarqm" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Data Desconto"
  MAX-WIDTH 4
  COLUMN-LABEL "Dt.Descon."
  VALEXP "titdtdes = ? or (titdtdes >= input titdtemi and titdtdes <= input titdtven)"
  VALMSG "Data p/Desconto deve ser maior que Emissao e menor que Vencim."
  HELP "Informe a Data Limite para pagamento com Desconto."
  ORDER 120

ADD FIELD "titvljur" OF "tituarqm" AS decimal 
  FORMAT ">,>>>,>>>,>>9.99"
