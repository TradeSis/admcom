  VALEXP "titnum <> """""
  VALMSG "Numero do Titulo nao pode ser brancos"
  HELP "Informe o Numero do Titulo."
  ORDER 50

ADD FIELD "titpar" OF "titulo" AS integer 
  FORMAT ">>9"
  INITIAL "1"
  LABEL "Parcela"
  MAX-WIDTH 4
  COLUMN-LABEL "PC"
  VALEXP "titpar > 0"
  VALMSG "Numero da Parcela do Titulo nao pode ser zero."
  HELP "Informe o Numero da Parcela do Titulo."
  ORDER 60

ADD FIELD "etbcod" OF "titulo" AS integer 
